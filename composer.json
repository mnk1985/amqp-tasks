{
    "name": "mnk1985/amqp-tasks-bundle",
    "type": "symfony-bundle",
    "description": "Simple Async Task Queue, Based On RabbitMQ Queues",
    "keywords": ["symfony", "rabbitmq", "queues", "tasks", "async"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/collections": "^1.5",
        "php-amqplib/php-amqplib": "^2.7",
        "symfony/framework-bundle": "^3|^4",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AmqpTasksBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    }
}
